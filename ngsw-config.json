{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
	"name": "players",
	"urls": [
		"https://wcfl.excelmec.org/v1/api/players"
	 ],
	 "cacheConfig": {
	   "maxSize": 1,
	   "maxAge": "8h",
           "timeout": "10s",
	   "strategy": "freshness"
    	}
    }
  ]
}
